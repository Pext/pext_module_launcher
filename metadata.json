{
  "id": "pext.module.launcher",
  "name": "App Launcher",
  "developer": "Sylvia van Os (Pext lead developer)",
  "description": "Allows Pext to launch applications from your computer.",
  "homepage": "https://pext.hackerchick.me/",
  "license": "GPL-3.0+",
  "git_urls": [
    "https://github.com/Pext/pext_module_launcher"
  ],
  "git_branch_stable": "stable",
  "bugtracker": "https://github.com/Pext/pext_module_launcher",
  "bugtracker_type": "github",
  "settings": [{
    "name": "use_path",
    "description": "Use $PATH as primary source of applications instead of as fallback.",
    "options": ["Yes", "No"],
    "default": "No"
  }],
  "platforms": [
    "Linux",
    "Darwin",
    "Windows"
  ]
}
